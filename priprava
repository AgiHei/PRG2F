import java.util.Random;

public class HodMinci {
    public static void main(String[] args) {
        Random rng = new Random();
        int hodMinci = rng.nextInt(2);

        if (hodMinci == 0) {
            System.out.println("Orel");
        } else {
            System.out.println("Panna");
        }
    }
}
